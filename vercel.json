{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@now/node"
      }
    ],
    "buildCommand": "next build",
    "cleanUrls": false,
    "devCommand": "next dev",
    "framework": "nextjs",
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ]
  }